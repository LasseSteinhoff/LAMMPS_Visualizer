        <ui:TextField picking-mode="Ignore" value="filler text" name="Coordinates" label="X Y Z Coordinates" focusable="true" text="0 0 0" style="position: absolute; right: 20%; bottom: auto; top: 1%; visibility: visible; display: flex; left: 0; color: rgb(0, 0, 0); background-color: rgb(255, 255, 255); overflow: visible;" />
        <ui:Button text="Apply" display-tooltip-when-elided="true" name="ApplyCoordinates" style="display: flex; position: absolute; left: 80%; bottom: auto; right: 1%; top: 1%;" />
        <ui:Button text="Open Resource" display-tooltip-when-elided="true" name="OpenResource" style="position: absolute; top: 5%; right: 0; left: 0;" />
        <ui:Button text="Load All Time Steps" display-tooltip-when-elided="true" name="LoadAllTimeSteps" style="position: absolute; top: 30%; right: 0; left: 0;" />
        <ui:Button text="Reload Data" display-tooltip-when-elided="true" name="ReloadData" style="position: absolute; top: 15%; right: 0; left: 0;" />
        <ui:Button text="Linear Interpolation" display-tooltip-when-elided="true" name="Lerping" style="position: absolute; top: 20%; right: 0; left: 0;" />
        <ui:Button text="Create Bonds" display-tooltip-when-elided="true" name="CreateBonds" style="position: absolute; top: 60%; right: 0; left: 0;" />
        <ui:TextField picking-mode="Ignore" name="BindingDistanceInput" label="Binding Distance" focusable="true" text="100" style="position: absolute; right: 0; bottom: auto; top: 65%; visibility: visible; display: none; left: 0; color: rgb(0, 0, 0); background-color: rgb(255, 255, 255); overflow: visible;" />
        <ui:Button text="Import Bonds" display-tooltip-when-elided="true" name="ImportBonds" style="position: absolute; top: 90%; right: 0; left: 0;" />
        <ui:Button text="Visibility Switch" display-tooltip-when-elided="true" name="VisibilitySwitch" style="position: absolute; top: 10%; right: 0; left: 0;" />
