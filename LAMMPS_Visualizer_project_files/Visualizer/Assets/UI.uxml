        <ui:TextField picking-mode="Ignore" name="CubePosition" label="Cube Position" focusable="true" text="0 0 0" style="position: absolute; right: 0; bottom: auto; top: 45%; visibility: visible; display: none; left: 0; color: rgb(0, 0, 0); background-color: rgb(255, 255, 255); overflow: visible;" />
        <ui:Button text="Slice" display-tooltip-when-elided="true" name="Slice" style="position: absolute; top: 55%; left: 0; height: auto; width: auto; bottom: auto; align-items: auto; right: 50%; justify-content: flex-start; display: none;" />
        <ui:Button text="Apply Minimize" display-tooltip-when-elided="true" name="ApplyMinimize" focusable="false" style="display: none; position: absolute; left: 0; bottom: auto; right: 0; top: 75%; overflow: visible;" />
        <ui:Button text="Analyse Bonds" display-tooltip-when-elided="true" name="AnalyseBonds" style="position: absolute; top: 85%; right: 0; bottom: auto; display: none; left: 0; overflow: visible;" />
        <ui:Button text="Adjust" display-tooltip-when-elided="true" name="AdjustSlice" style="position: absolute; top: 55%; left: 50%; height: auto; width: auto; bottom: auto; align-items: auto; right: 0; justify-content: flex-start; display: none;" />
