<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <ui:Button text="X" display-tooltip-when-elided="true" name="Escape" style="position: absolute; top: 1%; left: auto; height: 33px; width: 33px; align-items: auto; justify-content: space-between; right: 1%; font-size: 20px;" />
    <ui:Label display-tooltip-when-elided="true" name="InfoLabel" style="position: absolute; left: 20%; bottom: auto; font-size: 20px; -unity-font-style: bold; color: rgb(255, 254, 254); top: 1%; right: 30%;" />
    <ui:Label display-tooltip-when-elided="true" name="Timestep_Label" style="align-items: center; width: auto; height: auto; justify-content: flex-start; -unity-text-align: middle-center; white-space: nowrap; position: absolute; top: 0; left: 0; font-size: 24px; -unity-background-image-tint-color: rgb(0, 0, 0); background-color: rgb(0, 0, 0); margin-left: 0; margin-right: 0; margin-top: 0; margin-bottom: 0; padding-left: 10px; padding-right: 10px; padding-top: 3px; padding-bottom: 3px; color: rgb(255, 255, 255); bottom: 95%; right: auto;" />
