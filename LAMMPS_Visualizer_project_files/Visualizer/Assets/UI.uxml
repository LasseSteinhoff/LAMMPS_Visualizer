        <ui:ScrollView scroll-deceleration-rate="0,135" elasticity="0,1" name="Combinations" style="position: absolute; top: 5%; bottom: 0; right: 50%; display: flex; left: 0;" />
        <ui:ScrollView horizontal-scroller-visibility="Hidden" name="Structures" style="position: absolute; bottom: 0; right: 0; align-items: auto; top: 5%; display: flex; left: 50%;" />
        <ui:Label text="Protein Combination" display-tooltip-when-elided="true" name="ProteinCombination" style="position: absolute; right: 50%; top: auto; font-size: 16px; color: rgb(255, 255, 255); -unity-font-style: bold; left: auto;" />
    </ui:VisualElement>
    <ui:ScrollView name="Resource_ScrollView" style="position: absolute; left: 20%; right: 20%; top: 20%; bottom: 20%; display: none;" />
    <ui:ScrollView name="Proteins" horizontal-scroller-visibility="Hidden" style="left: 1%; top: 5%; width: 15%; height: 70%; bottom: auto; position: absolute; display: flex; right: auto;" />
