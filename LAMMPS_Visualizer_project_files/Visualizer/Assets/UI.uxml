        <ui:Button text="Apply Format" display-tooltip-when-elided="true" name="BindingDistanceButton" style="display: none; position: absolute; left: 0; bottom: auto; right: 0; top: 80%; overflow: visible;" />
        <ui:Slider picking-mode="Ignore" value="0" high-value="0" name="Timestep_Slider" low-value="0" show-input-field="false" style="left: 0; bottom: auto; top: 35%; width: auto; display: none; font-size: 20px; color: rgb(255, 255, 255); visibility: visible; position: absolute; right: 0;" />
